{
	"name": "Department",
	"targetDef": {
		"query":"SELECT ?x WHERE {?x a ub:Department}",
		"class": "ub:Department"
	},
	"prefix": {
		"ub": "<http://swat.cse.lehigh.edu/onto/univ-bench.owl#>"
	},
	"constraintDef": {
		"conjunctions": [
			[
				{ "path": "ub:name", "min":  1},
				{ "path": "ub:name", "max":  1},
				{ "path": "ub:subOrganizationOf", "min": 1, "shape": "University" },
				{ "path": "ub:subOrganizationOf", "max": 1, "shape": "University" }
            ]
		]
	}
}
